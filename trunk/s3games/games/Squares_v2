[EXPRESSIONS]

CheckUpper
    NOT EMPTY("gv($L,$M0)")
    NOT EMPTY("gh($L,$M0)")
    NOT EMPTY("gv($L1,$M0)")
    $SCORE = $SCORE + 1
    SETSTATE("sqr($L,$M0)",PLAYER)    
END

CheckLower
    NOT EMPTY("gv($L,$M)")
    NOT EMPTY("gh($L,$M1)")
    NOT EMPTY("gv($L1,$M)")
    $SCORE = $SCORE + 1
    SETSTATE("sqr($L,$M)",PLAYER)
END

CheckLeft
    NOT EMPTY("gh($L0,$M)")
    NOT EMPTY("gh($L0,$M1)")
    NOT EMPTY("gv($L0,$M)")
    $SCORE = $SCORE + 1
    SETSTATE("sqr($L0,$M)",PLAYER)
END

CheckRight
    NOT EMPTY("gh($L,$M)")
    NOT EMPTY("gh($L,$M1)")
    NOT EMPTY("gv($L1,$M)")
    $SCORE = $SCORE + 1
    SETSTATE("sqr($L,$M)",PLAYER)
END

CheckSquareH
    $SCORE = 0
    $M0 = $M - 1
    $M1 = $M + 1
    $L1 = $L + 1
    CheckUpper OR TRUE  
    CheckLower OR TRUE
    IF($SCORE==0,NEXTPLAYER,true)
END

CheckSquareV
    $SCORE = 0
    $M1 = $M + 1
    $L1 = $L + 1
    $L0 = $L - 1
    CheckLeft OR TRUE  
    CheckRight OR TRUE
    IF($SCORE==0,NEXTPLAYER,true)
END

FullBoard
    FORALL($A, 1, 5, FORALL($B, 1, 6, (NOT EMPTY("gh($A,$B)")) ))
    FORALL($A, 1, 6, FORALL($B, 1, 5, (NOT EMPTY("gv($A,$B)")) ))
END
END

[SCORINGS]
situation=true
player=PLAYER
score=$SCORE

[END OF GAME]

    situation=FullBoard  AND SCORE(1)>SCORE(2)
    winner=1    
    
    situation=FullBoard  AND SCORE(2)>SCORE(1)
    winner=2   
    
    situation=FullBoard  AND SCORE(1) == SCORE(2)
    winner=0

[GAME RULES]

name=redstepV
element=redV($J)
player=1
from=srv($K)
to=gv($L,$M)
followup=CheckSquareV

name=redstepH
element=redH($J)
player=1
from=srh($K)
to=gh($L,$M)
followup=CheckSquareH

name=bluestepV
element=blueV($J)
player=2
from=sbv($K)
to=gv($L,$M)
followup=CheckSquareV

name=bluestepH
element=blueH($J)
player=2
from=sbh($K)
to=gh($L,$M)
followup=CheckSquareH